	public void setQuoteDAO(QuoteDAO quoteDAO) {
		this.quoteDAO = quoteDAO;
	}


	@Override
	public void getRandomQuote() {
//		quoteDAO.getRandomQuote();
		quoteDAO.getLastId();
	}

}
